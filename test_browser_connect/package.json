{
    "name": "test-browser-connect",
    "displayName": "Browser AI Studio Connect",
    "description": "Connect to AI Studio via Browser",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.90.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "browser-connect.initialize",
                "title": "Browser Connect: Initialize AI Studio",
                "category": "AI"
            }
        ],
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "browser-connect-container",
                    "title": "AI Studio Chat",
                    "icon": "$(comment-discussion)"
                }
            ]
        },
        "views": {
            "browser-connect-container": [
                {
                    "id": "browser-connect-chat",
                    "name": "Chat",
                    "type": "webview"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./"
    },
    "dependencies": {
        "puppeteer": "^21.0.0",
        "ws": "^8.14.0"
    },
    "devDependencies": {
        "@types/vscode": "^1.90.0",
        "@types/node": "20.x",
        "@types/ws": "^8.5.0",
        "typescript": "^5.4.5"
    }
}